<%
  main_header_title = "GRADERS APP"
  main_header_link = "home"
  
  home = {title: "Student Home", reference: "home"}
  a = Schedule.find_by(user_id: current_user)
  if(a != nil)
  	submit_application = {title: "Edit Application", reference: "grader_application"}
  else
  	submit_application = {title: "New Application", reference: "grader_application"}
  end

  side_nav_links = [home, submit_application]
%>
<%=
  render partial: "layouts/side_nav", locals: {side_nav_header_title: main_header_title, side_nav_header_link: main_header_link, side_nav_links: side_nav_links}
%>